<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TechnicalAssessment</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TechnicalAssessment.styles.css" asp-append-version="true" />
</head>
<body>
  <div class="sidebar d-none d-md-block">
    <ul class="nav flex-column">
      <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Students</a>
      </li>
      <li class="nav-item"></li>
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Lorem Ipsum</a>
      </li>
      <li class="nav-item"></li>
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Dolor Sit</a>
      </li>
    </ul>
  </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

 
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
